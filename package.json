{
  "name": "ai-planner",
  "type": "module",
  "version": "0.0.1",
  "scripts": {
    "dev": "astro dev --port 3000 --host",
    "build": "astro build",
    "preview": "astro preview --port 3000 --host",
    "astro": "astro",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "format": "prettier --write .",
    "test": "vitest run",
    "test:watch": "vitest",
    "test:ui": "vitest --ui",
    "test:coverage": "vitest run --coverage",
    "test:e2e": "dotenv -e .env.test -- cross-env NODE_OPTIONS=\"--no-node-snapshot\" playwright test e2e/meeting-proposals-e2e-test.spec.ts",
    "test:e2e:ui": "dotenv -e .env.test -- cross-env NODE_OPTIONS=\"--no-node-snapshot\" playwright test --ui",
    "test:e2e:file": "dotenv -e .env.test -- cross-env NODE_OPTIONS=\"--no-node-snapshot\" playwright test",
    "test:e2e:all": "dotenv -e .env.test -- node ./e2e/run-tests.js",
    "e2e:dev": "dotenv -e .env.test -- astro dev --port 3000 --host --mode test"
  },
  "dependencies": {
    "@astrojs/node": "^7.0.4",
    "@astrojs/react": "^3.0.9",
    "@astrojs/tailwind": "^5.1.0",
    "@hookform/resolvers": "^3.10.0",
    "@radix-ui/react-alert-dialog": "^1.0.5",
    "@radix-ui/react-checkbox": "^1.0.4",
    "@radix-ui/react-dialog": "^1.1.13",
    "@radix-ui/react-label": "^2.1.6",
    "@radix-ui/react-popover": "^1.0.7",
    "@radix-ui/react-radio-group": "^1.1.3",
    "@radix-ui/react-select": "^2.0.0",
    "@radix-ui/react-slot": "^1.2.2",
    "@radix-ui/react-toast": "^1.2.13",
    "@supabase/auth-helpers-react": "^0.4.2",
    "@supabase/ssr": "^0.1.0",
    "@supabase/supabase-js": "^2.39.7",
    "astro": "^4.1.1",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "date-fns": "^3.3.1",
    "dotenv": "^16.3.1",
    "lucide-react": "^0.358.0",
    "openai": "^4.28.4",
    "react": "^18.2.0",
    "react-day-picker": "^8.10.0",
    "react-dom": "^18.2.0",
    "react-hook-form": "^7.56.3",
    "sonner": "^2.0.3",
    "tailwind-merge": "^2.6.0",
    "tailwindcss": "^3.4.1",
    "tailwindcss-animate": "^1.0.7",
    "zod": "^3.24.4"
  },
  "devDependencies": {
    "@eslint/compat": "^1.2.9",
    "@playwright/test": "^1.52.0",
    "@testing-library/jest-dom": "^6.6.3",
    "@testing-library/react": "^16.3.0",
    "@testing-library/user-event": "^14.6.1",
    "@types/node": "^20.11.25",
    "@types/react": "^18.2.64",
    "@types/react-dom": "^18.2.21",
    "@typescript-eslint/eslint-plugin": "^7.18.0",
    "@typescript-eslint/parser": "^7.18.0",
    "@vitest/coverage-v8": "^3.1.3",
    "cross-env": "^7.0.3",
    "dotenv-cli": "^8.0.0",
    "eslint": "^8.57.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-astro": "^0.31.4",
    "eslint-plugin-jsx-a11y": "^6.8.0",
    "eslint-plugin-prettier": "^5.4.0",
    "eslint-plugin-react": "^7.34.0",
    "eslint-plugin-react-compiler": "^19.1.0-rc.1",
    "eslint-plugin-react-hooks": "^4.6.0",
    "happy-dom": "^17.4.7",
    "husky": "^9.0.11",
    "jsdom": "^26.1.0",
    "lint-staged": "^15.2.2",
    "prettier": "^3.2.5",
    "prettier-plugin-astro": "^0.13.0",
    "typescript": "^5.4.2",
    "typescript-eslint": "^8.32.1",
    "vitest": "^3.1.3"
  },
  "lint-staged": {
    "*.{ts,tsx,astro}": [
      "eslint --fix"
    ],
    "*.{json,css,md}": [
      "prettier --write"
    ]
  },
  "dev:e2e": "astro dev --mode test"
}
